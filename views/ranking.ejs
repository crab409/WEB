<!DOCTYPE html>
<html>
    <head>
        <title>ALZalz | rank</title>
        <meta charset="utf-8">
        <link rel="stylesheet" href="/css/baseCss.css">
        <link rel="stylesheet" href="/css/ranking.css">
    </head>
    <body>
        <%- include('blockHeader.ejs') %>


        <div id="base-main">
            <div class="side-bar">
            </div>
            <div class="main">
                <div class="jumbo"><h2>랭킹</h2></div>
                <div class="container">
                    <ul class="peopleDatas" style="padding: 0px;">
                        <% for(let i=0; i<peoplesData.length; i++) { %>
                            <a href="/content">
                            <li>
                                <strong style="display: flex; justify-content: space-between;">
                                    <div class="name">
                                        <%= peoplesData[i].username %> 
                                    </div>
                                    <div class="user_info">
                                        <%= JSON.stringify(peoplesData[i].clearCount) %> 
                                    </div>
                                </strong>
                            </li></a>
                        <% } %>
                    </ul>
                    
                </div>
            </div>
        </div>


        <%- include('blockFooter.ejs') %>
    </body>
</html>